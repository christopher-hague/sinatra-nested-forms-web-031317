<h1>Make your form here</h1>
<form action="/pirates" method="post">
  Pirate Name: <input type="text" name="pirate[name]">
  Pirate Weight <input type="text" name="pirate[weight]">
  Pirate Height: <input type="text" name="pirate[height]">
  Ship One Name: <input id="ship_name_1" type="text" name="pirate[ships][][name]">
  Ship One Type: <input id="ship_type_1" type="text" name="pirate[ships][][type]">
  Ship One Booty: <input id="ship_booty_1" type="text" name="pirate[ships][][booty]">
  Ship Two Name: <input id="ship_name_2" type="text" name="pirate[ships][][name]">
  Ship Two Type: <input id="ship_type_2" type="text" name="pirate[ships][][type]">
  Ship Two Booty: <input id="ship_booty_2" type="text" name="pirate[ships][][booty]">
  <input type="submit" name="submit" value="Submit">
</form>
